{
    "name": "assoconnect/php-i18n",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/php-i18n/"
        }
    ],
    "type": "library",
    "description": "I18n utils",
    "keywords": ["php", "i18n"],
    "homepage": "https://github.com/assoconnect/php-i18n/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AssoConnect\\PhpI18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssoConnect\\PhpI18n\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "roave/no-floaters": "^1.5",
        "assoconnect/php-quality-config": "^1.16"
    },
    "require": {
        "php": "^8.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
