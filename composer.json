{
    "name": "assoconnect/php-i18n",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/php-i18n/"
        }
    ],
    "type": "library",
    "description": "I18n utils",
    "keywords": ["php", "i18n"],
    "homepage": "https://github.com/assoconnect/php-i18n/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AssoConnect\\PhpI18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssoConnect\\PhpI18n\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "roave/no-floaters": "^1.5"
    },
    "require": {
        "php": "^7.4|^8.0"
    }
}
